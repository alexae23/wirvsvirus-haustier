<div class="row s12 l12 xl12 listLayout-group">
    <div class="listOverview">
        <!--some personal info for example address -->
    </div>
    <div class="list">
        {{#if isHelperSearch}}
            {{#each helperarr}}
                {{>helpersView helpers=this}}
            {{/each}}
        {{else}} <!-- must be offer search -->
            {{#each offerarr}}
                {{>offersView offers=this}}
            {{/each}}
        {{/if}}
    </div>
</div>
